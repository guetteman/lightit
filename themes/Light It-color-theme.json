{
	"name": "Light It",
	"type": "light",
	"colors": {
		"editor.background": "#F1F5F9", // Bluegray 100
		"editor.foreground": "#475569", // Bluegray 600
        "editor.findMatchBackground": "#FED7AA", // Orange 200
        "editor.findMatchHighlightBackground": "#FFEDD5", // Orange 100
        "editorCursor.background": "#F0F9FF", // Sky 50
        "editorCursor.foreground": "#0369A1", // Sky 700
        "editor.selectionBackground": "#FFEDD5", // Orange 100
        "editorLineNumber.foreground": "#94A3B8", // Bluegray 400
        "editorLineNumber.activeForeground": "#C084FC", // Purple 400
        "editorLink.activeForeground": "#818CF8", // Indigo 400
        "editorInfo.foreground": "#34D399",
        "editor.snippetTabstopHighlightBackground": "#FFEDD5", // Orange 100
        "terminal.selectionBackground": "#FFEDD5", // Orange 100
        "terminalCursor.background": "#F0F9FF", // Sky 50
        "terminalCursor.foreground": "#0369A1", // Sky 700
        "diffEditor.removedTextBackground": "#FEE2E2", // Red 100
        "diffEditor.insertedTextBackground": "#DCFCE7", // Green 100
        "sideBar.background": "#E2E8F0", // Bluegray 200
        "sideBarSectionHeader.background": "#CBD5E1", // Bluegray 300
        "sideBarSectionHeader.border": "#F0F9FF", // Sky 50
        "sideBarSectionHeader.foreground": "#64748B", // Bluegray 600
        "icon.foreground": "#0369A1", // Sky 700
        "list.activeSelectionBackground": "#F0F9FF", // Sky 50
        "list.activeSelectionForeground": "#0369A1", // Sky 700
        "list.activeSelectionIconForeground": "#0369A1", // Sky 700
        "list.hoverBackground": "#F0F9FF", // Sky 50
        "list.focusOutline": "#0369A1", // Sky 700
        "activityBar.background": "#075985", // Sky 800
        "button.background": "#0284C7", // Sky 600
        "button.hoverBackground": "#075985" // Sky 800
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#94A3B8" // Bluegray 400
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#94A3B8" // Bluegray 400
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#FBBF24"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#64748B" // Bluegray 500
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#0891B2" // Cyan 600
			}
		},
        {
			"name": "Keywords",
			"scope": [
				"keyword",
			],
			"settings": {
				"foreground": "#15803D" // Green 700
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#0891B2" // Cyan 600
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#D97706" // Amber 600
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#334155" // Bluegray 700
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#0369A1" // Sky 700
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
                "meta.class"
			],
			"settings": {
				"foreground": "#4D7C0F" // Lime 700
			}
		},
        {
			"name": "Inherited classes",
			"scope": [
                "support.other",
                "support.class"
			],
			"settings": {
				"foreground": "#64748B" // Bluegray 500
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#D97706" // Amber 600
			}
		},
		{
			"name": "Strings",
			"scope": [
                "string",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
			"settings": {
				"foreground": "#D97706" // Amber 600
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#D97706" // Amber 600
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#D97706" // Amber 600
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#334155" // Bluegray 700
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#6B7280" // Bluegray 500
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#0891B2" // Sky 600
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#15803D" // Green 700
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#0891B2" // Sky 600
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#059669" // Emerald 600
			}
		},
